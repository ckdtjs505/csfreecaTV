    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.8.3",
    "autoprefixer": "^9.7.3",
    "aws-sdk": "^2.608.0",
    "axios": "^0.19.1",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "css-loader": "^3.4.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-kakao": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "sass-loader": "^8.0.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build",
    "copyAll": "cp -R src/static build && cp -R src/views build && cp -R src/img build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "npm run build && node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1"
  }
}
